@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix dc:   <http://purl.org/dc/elements/1.1/> .
@prefix ga:   <https://github.com/orey/graphapps/grammars/meta-grammar.ttl#> .


<https://github.com/orey/graphapps#> a owl:Ontology ;
    dc:title "CSV2RDF conversion meta-grammar" ;
    dc:description "Basic primitives" .


ga:Line a rdf:Seq ;
        rdfs:isDefinedBy <https://github.com/orey/graphapps/grammars/meta-grammar.ttl#> ;
        rdfs:comment "The line to parse must be redeclared in the grammar file." .

ga:Identifier a rdfs:label ;
              rdfs:isDefinedBy <https://github.com/orey/graphapps/grammars/meta-grammar.ttl#> ;
              rdfs:comment "".

ga:Separator a rdfs:label ;
             rdfs:isDefinedBy <https://github.com/orey/graphapps/grammars/meta-grammar.ttl#>

